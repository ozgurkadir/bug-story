{
  "name": "bug-story",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap/audit-logging": "^3.1.0",
    "@sap/cds": "^3",
    "@sap/cds-odata-v2-adapter-proxy": "^1.4.41",
    "@sap/hana-client": "^2.5.86",
    "@sap/xsenv": "^3.0.0",
    "@sap/xssec": "^2.2.5",
    "dayjs": "^1.9.6",
    "express": "^4",
    "helmet": "^4.2.0",
    "passport": "^0.4.1",
    "passport-oauth2": "^1.5.0"
  },
  "scripts": {
    "startdev": "npx cds run",
    "start": "cds run",
    "test": "mocha test/project_tests.js --timeout 15000 --exit"
  },
  "cds": {
    "requires": {
      "uaa": {
        "kind": "xsuaa"
      },
      "db": {
        "kind": "sqlite",
        "credentials": {
          "database": "db/bugstory.db"
        }
      },
      "userExtService": {
        "kind": "rest",
        "credentials": {
          "destination": "userBugDestinationSrv",
          "path": "/Users",
          "requestTimeout": 30000
        },
        "pool": {
          "min": 1,
          "max": 10
        }
      }
    },
    "odata": {
      "version": "v4"
    }
  },
  "devDependencies": {
    "@sap/ux-specification": "^1.84.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "mocha": "^8.0.1",
    "sqlite3": "^5.0.0"
  },
  "sapux": [
    "app/project1",
    "app/usermanagement"
  ]
}